<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    tools:context=".ui.survey.SurveyResultActivity">

    <ProgressBar
        android:id="@+id/progressBar3"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="false"
        android:layout_centerInParent="true"
        android:progress="50"
        android:progressTint="@color/secondaryDarkColor"
        android:scaleY="10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <ProgressBar
        android:id="@+id/progressBar1"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="false"
        android:layout_centerInParent="true"
        android:layout_marginBottom="32dp"
        android:progress="50"
        android:progressTint="@color/secondaryDarkColor"
        android:scaleY="10"
        app:layout_constraintBottom_toTopOf="@+id/rbtv3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="false"
        android:layout_centerInParent="true"
        android:layout_marginBottom="32dp"
        android:progress="50"
        android:progressTint="@color/secondaryDarkColor"
        android:scaleY="10"
        app:layout_constraintBottom_toTopOf="@+id/rbtv1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ProgressBar
        android:id="@+id/progressBar4"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="false"
        android:layout_centerInParent="true"
        android:layout_marginTop="32dp"
        android:progress="50"
        android:progressTint="@color/secondaryDarkColor"
        android:scaleY="10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rbtv3" />

    <ProgressBar
        android:id="@+id/progressBar5"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="false"
        android:layout_centerInParent="true"
        android:layout_marginTop="32dp"
        android:progress="50"
        android:progressTint="@color/secondaryDarkColor"
        android:scaleY="10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rbtv4" />

    <ProgressBar
        android:id="@+id/progressBar6"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="false"
        android:layout_centerInParent="true"
        android:layout_marginTop="32dp"
        android:progress="50"
        android:progressTint="@color/secondaryDarkColor"
        android:scaleY="10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rbtv5" />

    <TextView
        android:id="@+id/rbtv3"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_alignLeft="@id/progressBar3"
        android:layout_alignTop="@id/progressBar3"
        android:layout_alignRight="@id/progressBar3"
        android:layout_alignBottom="@id/progressBar3"
        android:background="#00000000"
        android:gravity="center"
        android:text="Result"
        android:textColor="@android:color/background_light"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/progressBar3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/progressBar3"
        app:layout_constraintTop_toTopOf="@+id/progressBar3" />

    <TextView
        android:id="@+id/rbtv1"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_alignLeft="@id/progressBar3"
        android:layout_alignTop="@id/progressBar3"
        android:layout_alignRight="@id/progressBar3"
        android:layout_alignBottom="@id/progressBar3"
        android:background="#00000000"
        android:gravity="center"
        android:text="Result"
        android:textColor="@android:color/background_light"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/progressBar1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/progressBar3"
        app:layout_constraintTop_toTopOf="@+id/progressBar1" />

    <TextView
        android:id="@+id/rbtv"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_alignLeft="@id/progressBar3"
        android:layout_alignTop="@id/progressBar3"
        android:layout_alignRight="@id/progressBar3"
        android:layout_alignBottom="@id/progressBar3"
        android:background="#00000000"
        android:gravity="center"
        android:text="Result"
        android:textColor="@android:color/background_light"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/progressBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/progressBar3"
        app:layout_constraintTop_toTopOf="@+id/progressBar" />

    <TextView
        android:id="@+id/rbtv4"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_alignLeft="@id/progressBar3"
        android:layout_alignTop="@id/progressBar3"
        android:layout_alignRight="@id/progressBar3"
        android:layout_alignBottom="@id/progressBar3"
        android:background="#00000000"
        android:gravity="center"
        android:text="Result"
        android:textColor="@android:color/background_light"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/progressBar4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/progressBar3"
        app:layout_constraintTop_toTopOf="@+id/progressBar4" />

    <TextView
        android:id="@+id/rbtv5"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_alignLeft="@id/progressBar3"
        android:layout_alignTop="@id/progressBar3"
        android:layout_alignRight="@id/progressBar3"
        android:layout_alignBottom="@id/progressBar3"
        android:background="#00000000"
        android:gravity="center"
        android:text="Result"
        android:textColor="@android:color/background_light"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/progressBar5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/progressBar3"
        app:layout_constraintTop_toTopOf="@+id/progressBar5" />

    <TextView
        android:id="@+id/rbtv6"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_alignLeft="@id/progressBar3"
        android:layout_alignTop="@id/progressBar3"
        android:layout_alignRight="@id/progressBar3"
        android:layout_alignBottom="@id/progressBar3"
        android:background="#00000000"
        android:gravity="center"
        android:text="Result"
        android:textColor="@android:color/background_light"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/progressBar6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/progressBar3"
        app:layout_constraintTop_toTopOf="@+id/progressBar6" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@android:color/background_light"
        android:gravity="center_horizontal"
        android:text="TextView"
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="48dp"
        android:background="@color/secondaryLightColor"
        android:gravity="center_horizontal|center_vertical"
        android:padding="10dp"
        android:text="TextView"
        android:textColor="@color/primaryTextColor"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/rbtv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>